[global]
ioengine=libaio
direct=1
invalidate=1
fsync=0
runtime=30s
time_based
group_reporting
numjobs=4
iodepth=16
bs=4k
size=16M
rw=randread
write_bw_log=cephops
write_iops_log=cephops
write_lat_log=cephops
write_hist_log=cephops
#directory=/mnt/sheridan/fio

[randread]
rw=randread
stonewall

[randwrite]
rw=randwrite
stonewall

[seqread]
rw=read
bs=128k
stonewall

[seqwrite]
rw=write
bs=128k
